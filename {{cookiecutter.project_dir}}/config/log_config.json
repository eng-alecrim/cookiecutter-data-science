{
  "handlers": [
    {
      "sink": "log/info.log",
      "rotation": "1 week",
      "retention": "1 month",
      "level": "INFO",
      "format": "{time} | {level} | {message}"
    },
    {
      "sink": "log/debugging.log",
      "rotation": "1 week",
      "retention": "1 month",
      "level": "TRACE",
      "format": "{time} | {level} | {message}"
    },
    {
      "sink": "log/errors.log",
      "rotation": "1 week",
      "retention": "1 month",
      "level": "WARNING",
      "format": "{time} | {level} | {message}"
    }
  ]
}
